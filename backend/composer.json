{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "awcodes/filament-tiptap-editor": "^3.5",
        "awcodes/shout": "^2.1",
        "codewithdennis/filament-select-tree": "^3.1",
        "dutchcodingcompany/filament-socialite": "^2.4",
        "filament/filament": "3.3",
        "filament/spatie-laravel-media-library-plugin": "^3.3",
        "flowframe/laravel-trend": "^0.4.0",
        "kirschbaum-development/commentions": "^0.6.6",
        "laravel/framework": "^12.0",
        "laravel/mcp": "^0.1.1",
        "laravel/sanctum": "^4.0",
        "laravel/telescope": "^5.10",
        "laravel/tinker": "^2.10.1",
        "mintreu/laravel-category": "dev-main",
        "mintreu/laravel-commerinity": "dev-main",
        "mintreu/laravel-geokit": "dev-main",
        "mintreu/laravel-helpdesk": "dev-main",
        "mintreu/laravel-money": "dev-main",
        "mintreu/laravel-naukri-manager": "dev-main",
        "mintreu/laravel-penpress": "dev-main",
        "mintreu/laravel-product-catalogue": "dev-main",
        "mintreu/laravel-transaction": "dev-main",
        "mintreu/toolkit": "dev-main",
        "owenvoke/blade-fontawesome": "^2.9",
        "saade/filament-adjacency-list": "^3.2",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "staudenmeir/laravel-adjacency-list": "^1.25",
        "stechstudio/filament-impersonate": "^3.16"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "fakerphp/faker": "^1.23",
        "laravel/boost": "^1.1",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-livewire": "^4.0",
        "phpunit/phpunit": "12.3.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/mintreu/laravel-money",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/mintreu/toolkit",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/mintreu/laravel-category",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/mintreu/laravel-geokit",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/mintreu/laravel-product-catalogue",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/mintreu/laravel-naukri-manager",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/mintreu/laravel-penpress",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/mintreu/laravel-helpdesk",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/mintreu/laravel-integration",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/mintreu/laravel-transaction",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/mintreu/laravel-commerinity",
            "options": {
                "symlink": true
            }
        }
    ],

    "minimum-stability": "dev",
    "prefer-stable": true
}
