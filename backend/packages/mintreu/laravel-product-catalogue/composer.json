{
    "name": "mintreu/laravel-product-catalogue",
    "description": "This is my package laravel-product-catalogue",
    "keywords": [
        "mintreu",
        "laravel",
        "laravel-product-catalogue"
    ],
    "homepage": "https://github.com/mintreu/laravel-product-catalogue",
    "support": {
        "issues": "https://github.com/mintreu/laravel-product-catalogue/issues",
        "source": "https://github.com/mintreu/laravel-product-catalogue"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Krishanu",
            "email": "36711704+krishzzi@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "awcodes/filament-table-repeater": "^3.1",
        "awcodes/filament-tiptap-editor": "^3.0",
        "awcodes/shout": "^2.1",
        "codewithdennis/filament-select-tree": "^3.1",
        "filament/filament": "^3.0",
        "filament/spatie-laravel-media-library-plugin": "^3.3",
        "illuminate/contracts": "*",
        "mintreu/laravel-category": "dev-main",
        "mintreu/laravel-geokit": "dev-main",
        "mintreu/laravel-money": "dev-main",
        "mintreu/toolkit": "dev-main",
        "spatie/laravel-package-tools": "^1.15.0",
        "staudenmeir/laravel-adjacency-list": "^1.25"
    },

    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^10.0.0||^9.0.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "spatie/laravel-ray": "^1.35"
    },


    "autoload": {
        "psr-4": {
            "Mintreu\\LaravelProductCatalogue\\": "src/",
            "Mintreu\\LaravelProductCatalogue\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mintreu\\LaravelProductCatalogue\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mintreu\\LaravelProductCatalogue\\LaravelProductCatalogueServiceProvider"
            ],
            "aliases": {
                "LaravelProductCatalogue": "Mintreu\\LaravelProductCatalogue\\Facades\\LaravelProductCatalogue"
            }
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../toolkit",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../laravel-category",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../laravel-money",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../laravel-geokit",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
