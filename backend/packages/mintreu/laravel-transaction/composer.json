{
    "name": "mintreu/laravel-transaction",
    "description": "This is my package laravel-transaction",
    "keywords": [
        "mintreu",
        "laravel",
        "laravel-transaction"
    ],
    "homepage": "https://github.com/mintreu/laravel-transaction",
    "support": {
        "issues": "https://github.com/mintreu/laravel-transaction/issues",
        "source": "https://github.com/mintreu/laravel-transaction"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Krishanu",
            "email": "36711704+krishzzi@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "mintreu/laravel-money": "dev-main",
        "mintreu/laravel-integration": "dev-main",
        "mintreu/toolkit": "dev-main",
        "simplesoftwareio/simple-qrcode": "^4.2"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Mintreu\\LaravelTransaction\\": "src/",
            "Mintreu\\LaravelTransaction\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mintreu\\LaravelTransaction\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mintreu\\LaravelTransaction\\LaravelTransactionServiceProvider"
            ],
            "aliases": {
                "LaravelTransaction": "Mintreu\\LaravelTransaction\\Facades\\LaravelTransaction"
            }
        }
    },

    "repositories": [
        {
            "type": "path",
            "url": "../toolkit",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../laravel-integration",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../laravel-money",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
