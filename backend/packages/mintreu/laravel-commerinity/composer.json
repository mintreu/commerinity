{
    "name": "mintreu/laravel-commerinity",
    "description": "Laravel based shop marketplace ecommerce solution",
    "keywords": [
        "mintreu",
        "laravel",
        "laravel-commerinity"
    ],
    "homepage": "https://github.com/mintreu/laravel-commerinity",
    "support": {
        "issues": "https://github.com/mintreu/laravel-commerinity/issues",
        "source": "https://github.com/mintreu/laravel-commerinity"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Krishanu",
            "email": "36711704+krishzzi@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "mintreu/laravel-category": "dev-main",
        "mintreu/laravel-money": "dev-main",
        "mintreu/toolkit": "dev-main",
        "mintreu/laravel-geokit": "dev-main",
        "mintreu/laravel-product-catalogue": "dev-main",
        "mintreu/laravel-transaction": "dev-main",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^10.0.0||^9.0.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "Mintreu\\LaravelCommerinity\\": "src/",
            "Mintreu\\LaravelCommerinity\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mintreu\\LaravelCommerinity\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mintreu\\LaravelCommerinity\\LaravelCommerinityServiceProvider"
            ],
            "aliases": {
                "LaravelCommerinity": "Mintreu\\LaravelCommerinity\\Facades\\LaravelCommerinity"
            }
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../toolkit",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../laravel-category",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../laravel-money",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../laravel-geokit",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../laravel-product-catalogue",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../laravel-transaction",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../laravel-integration",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
